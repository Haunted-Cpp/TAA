
\documentclass{article}
\setlength{\unitlength}{0,5cm}
\begin{document}
\begin{picture}(200,30)
\put(3,-1){\line(0,-1){1}}
\put(3,-2){\line(-1,0){1}}
\put(2,-2){\line(0,-1){1}}
\put(2,-3){\line(-1,0){1}}
\put(1,-3){\line(0,-1){1}}
\put(1,-4){\line(1,0){3}}
\put(4,-4){\line(0,1){3}}
\put(4,-1){\line(-1,0){1}}
\end{picture}
\end{document}
